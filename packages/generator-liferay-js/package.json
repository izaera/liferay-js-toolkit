{
	"name": "generator-liferay-js",
	"version": "2.10.0",
	"description": "Yeoman generators for Liferay DXP and Portal CE JavaScript projects.",
	"main": "generators/app/index.js",
	"files": [
		"generators"
	],
	"keywords": [
		"yeoman-generator",
		"liferay",
		"liferay-js"
	],
	"scripts": {
		"build": "node ./scripts/copy-templates && babel --source-maps -D -d generators src --ignore 'src/*/templates/**/*' --ignore '**/__tests__/**'",
		"prepublish": "npm run build"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-preset-env": "^1.7.0",
		"mem-fs": "^1.1.3",
		"mem-fs-editor": "^5.1.0",
		"ncp": "^2.0.0",
		"rimraf": "^2.6.2",
		"yargs": "^13.2.0"
	},
	"dependencies": {
		"dot-prop": "^5.0.0",
		"read-json-sync": "^2.0.0",
		"yeoman-generator": "^3.1.1"
	}
}

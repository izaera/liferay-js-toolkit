    "babel-template": "^6.26.0",
    "cpr": "^3.0.1",
    "dot-prop": "^4.2.0",
    "ejs": "^2.6.1",
    "fs-extra": "^7.0.0",
    "liferay-npm-build-tools-common": "2.10.0",
    "ncp": "^2.0.0",
    "properties": "^1.2.1",
    "read-json-sync": "^2.0.0",
    "request": "^2.88.0",
    "resolve": "^1.11.0",
    "uuid": "^3.3.2"
  }
}

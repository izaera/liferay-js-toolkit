{
	"/": {
		"plugins": ["resolve-linked-dependencies"],
		".babelrc": {
			"presets": ["liferay-standard"]
		},
		"post-plugins": ["create-react-app"]
	},
	"create-jar": {
		"output-dir": "build.liferay",
		"features": {
			"js-extender": true
		}
	},
	"exclude": {
		"*": ["**/*"]
	},
	"ignore": ["react-app/**"],
	"no-tracking": true,
	"output": "build.liferay/jar"
}
